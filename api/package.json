{
  "name": "nft-dex-api",
  "version": "1.0.0",
  "description": "NFT DEX REST API Server using Fastify",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test:api": "./scripts/test-api.sh",
    "test:quick": "./scripts/quick-test.sh",
    "test:bytecode": "ts-node scripts/test-bytecode-deployment.ts",
    "test:bytecode:deploy": "ts-node scripts/test-bytecode-deployment.ts --deploy",
    "demo:bytecode": "ts-node scripts/demo-bytecode-deployment.ts",
    "demo:bytecode:deploy": "ts-node scripts/demo-bytecode-deployment.ts --deploy",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "nft",
    "dex",
    "fastify",
    "ethereum",
    "api"
  ],
  "author": "NFT DEX Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.1",
    "@types/web3": "^1.2.2",
    "axios": "^1.12.2",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "fastify": "^4.24.3",
    "pino-pretty": "^10.2.3",
    "readline": "^1.3.0",
    "web3": "^4.16.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
